  "dependencies": {
    "@mapbox/geojson-extent": "^0.3.2",
    "@nivo/bar": "^0.42.1",
    "@nivo/line": "^0.42.1",
    "@turf/turf": "^5.1.6",
    "d3-array": "^1.2.4",
    "d3-collection": "^1.0.7",
    "d3-color": "^1.2.0",
    "d3-dsv": "^1.0.8",
    "d3-ease": "^1.0.3",
    "d3-format": "^1.3.0",
    "d3-geo": "^1.10.0",
    "d3-geo-projection": "^2.4.0",
    "d3-scale": "^2.0.0",
    "d3-scale-chromatic": "^1.3.0",
    "d3-selection": "^1.3.0",
    "deck.gl": "^5.2.3",
    "deep-equal": "^1.0.1",
    "falcor": "^2.0.1",
    "falcor-http-datasource": "^0.1.3",
    "history": "^4.7.2",
    "lodash.get": "^4.4.2",
    "mapbox-gl": "^0.51.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-map-gl": "^3.2.7",
    "react-markdown-renderer": "^1.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "simple-statistics": "^6.1.0",
    "topojson": "^3.0.2",
    "viewport-mercator-project": "^5.2.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "post-build": "react-snap",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "reactSnapshot": {
    "include": [],
    "exclude": [
      "/login",
      "/hazards/*"
    ]
  },
  "devDependencies": {
    "react-snap": "^1.19.0"
  },
  "reactSnap": {
    "puppeteer": {
      "cache": false
    },
    "skipThirdPartyRequests": false,
    "concurrency": 1
  }
}
